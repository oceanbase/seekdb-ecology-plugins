{
  "name": "seekdb-for-cursor",
  "displayName": "seekdb",
  "description": "Add seekdb database documentation to .cursor/rules directory, enabling Cursor AI assistant to understand seekdb database knowledge",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "seekdb-docs.copyToRules",
        "title": "Add seekdb Docs"
      },
      {
        "command": "seekdb-docs.removeFromRules",
        "title": "Remove seekdb Docs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "publisher": "davidzhang",
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanbase/cursor-extension"
  },
  "license": "MIT"
}

